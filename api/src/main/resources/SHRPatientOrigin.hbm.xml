<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.openmrs.module.sharedhealthrecord.SHRPatientOrigin" table="shr_patient_origin" lazy="false">
		  <id column="origin_id" name="originId" type="int">
            <generator class="native"/>
       	  </id>
       	  <property name="patient_uuid" column="patient_uuid" type="java.lang.String"/>
       	  <property name="patient_origin" column="patient_origin" type="java.lang.String"/>
       	  <property name="isSendToDHis" column="is_send_to_dhis" type="java.lang.Integer"/> 
       	  <property name="encounter_uuid" column="encounter_uuid" type="java.lang.String"/> 
       	   
	</class>
</hibernate-mapping>