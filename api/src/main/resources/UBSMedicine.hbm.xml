<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.openmrs.module.sharedhealthrecord.UBSMedicines" table="ubs_medicines" lazy="false">
		  <id column="medicine_id" name="medicineId" type="int">
            <generator class="native"/>
       	  </id>
       	  <property name="name" column="name" type="java.lang.String"/>
       	  <property name="code" column="code" type="java.lang.String"/>
       	  <property name="brandName" column="brandName" type="java.lang.String"/> 
       	  <property name="category" column="category" type="java.lang.String"/> 
       	  <property name="voided" column="voided" type="boolean" not-null="true"/>
       	   
	</class>
</hibernate-mapping>